<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.slideshow.VisualizarDadosCliente">
    <EditText
            android:layout_width="79dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="UF"
            android:ems="10"
            android:id="@+id/editText_UF2"
            tools:layout_conversion_absoluteHeight="42dp"
            tools:layout_conversion_absoluteWidth="101dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/editText_Cidade2"
            app:layout_constraintHorizontal_bias="0.469"
            app:layout_constraintTop_toBottomOf="@+id/editText_NumeroCasa2"
            android:layout_marginTop="88dp"/>
    <EditText
            android:layout_width="196dp"
            android:layout_height="43dp"
            android:inputType="date"
            android:hint="Data Nascimento"
            android:ems="10"
            android:id="@+id/editText_NumeroDocumento2"
            tools:layout_conversion_absoluteHeight="45dp"
            tools:layout_conversion_absoluteWidth="175dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editText_Nome2" android:layout_marginTop="140dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.055"/>
    <EditText
            android:layout_width="322dp"
            android:layout_height="39dp"
            android:inputType="textPersonName"
            android:hint="Nome Completo"
            android:ems="10"
            android:id="@+id/editText_Nome2"
            tools:layout_conversion_absoluteHeight="45dp"
            tools:layout_conversion_absoluteWidth="213dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.134" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.023"/>
    <EditText
            android:layout_width="94dp"
            android:layout_height="45dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:hint="Numero"
            android:id="@+id/editText_NumeroCasa2"
            tools:layout_conversion_absoluteHeight="45dp"
            tools:layout_conversion_absoluteWidth="81dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.861"
            app:layout_constraintTop_toBottomOf="@+id/editText_NumeroDocumento2" android:layout_marginTop="44dp"/>
    <EditText
            android:layout_width="221dp"
            android:layout_height="45dp"
            android:inputType="phone"
            android:ems="10"
            android:hint="Celular"
            android:id="@+id/editText_Celular2"
            tools:layout_conversion_absoluteHeight="45dp"
            tools:layout_conversion_absoluteWidth="213dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.068" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.027" app:layout_constraintTop_toBottomOf="@+id/editText_Nome2"/>
    <EditText
            android:layout_width="218dp"
            android:layout_height="40dp"
            android:inputType="phone"
            android:hint="Telefone Fixo"
            android:ems="10"
            android:id="@+id/editText_Telefone2"
            tools:layout_conversion_absoluteHeight="45dp"
            tools:layout_conversion_absoluteWidth="213dp"
            app:layout_constraintTop_toBottomOf="@+id/editText_Celular2" android:layout_marginTop="20dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.062"/>
    <EditText
            android:layout_width="178dp"
            android:layout_height="44dp"
            android:inputType="textPersonName"
            android:hint="Endereço"
            android:ems="10"
            android:id="@+id/editText_Endereco2"
            tools:layout_conversion_absoluteHeight="45dp"
            tools:layout_conversion_absoluteWidth="213dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.051"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.349" app:layout_constraintTop_toTopOf="parent"/>
    <EditText
            android:layout_width="261dp"
            android:layout_height="45dp"
            android:inputType="textPersonName"
            android:hint="Bairro"
            android:ems="10"
            android:id="@+id/editText_Bairro2"
            tools:layout_conversion_absoluteHeight="45dp"
            tools:layout_conversion_absoluteWidth="213dp"
            app:layout_constraintTop_toBottomOf="@+id/editText_Endereco2" android:layout_marginTop="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="12dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.031"/>
    <EditText
            android:layout_width="237dp"
            android:layout_height="47dp"
            android:inputType="textPersonName"
            android:hint="Cidade"
            android:ems="10"
            android:id="@+id/editText_Cidade2"
            tools:layout_conversion_absoluteHeight="45dp"
            tools:layout_conversion_absoluteWidth="213dp"
            app:layout_constraintTop_toBottomOf="@+id/editText_Bairro2" android:layout_marginTop="8dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.068" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.027"/>
    <EditText
            android:layout_width="296dp"
            android:layout_height="44dp"
            android:inputType="textPostalAddress"
            android:ems="10"
            android:hint="CEP"
            android:id="@+id/editText_CEP2"
            tools:layout_conversion_absoluteHeight="45dp"
            tools:layout_conversion_absoluteWidth="213dp"
            app:layout_constraintTop_toBottomOf="@+id/editText_Cidade2"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.104"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.029"/>
    <Button
            android:text="Salvar Alterações"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/buttonSalvarAlteracoesCliente"
            app:layout_constraintTop_toBottomOf="@+id/editText_CEP2"
            android:layout_marginTop="48dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.751" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.497"/>
</androidx.constraintlayout.widget.ConstraintLayout>