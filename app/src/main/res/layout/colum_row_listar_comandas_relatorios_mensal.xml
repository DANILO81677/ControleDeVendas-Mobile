<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent" android:layout_height="match_parent">
    <TextView android:id="@+id/mensal_relatorio_lista_mes"
              android:layout_height="wrap_content"
              android:layout_width="0dp"
              android:layout_weight="1"
    />
    <TextView android:id="@+id/mensal_relatorio_lista_ano"
              android:layout_height="wrap_content"
              android:layout_width="0dp"
              android:layout_weight="1"
    />
    <TextView android:id="@+id/mensal_relatorio_lista_despesa"
              android:layout_height="wrap_content"
              android:layout_width="0dp"
              android:layout_weight="1"
    />
    <TextView android:id="@+id/mensal_relatorio_lista_receita"
              android:layout_height="wrap_content"
              android:layout_width="0dp"
              android:layout_weight="1"
    />
    <TextView android:id="@+id/mensal_relatorio_lista_faturado"
              android:layout_height="wrap_content"
              android:layout_width="0dp"
              android:layout_weight="1"
    />
</LinearLayout>