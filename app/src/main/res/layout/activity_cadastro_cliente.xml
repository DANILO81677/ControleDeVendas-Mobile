<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.slideshow.CadastroCliente">

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="phone"
            android:ems="10"
            android:hint="Celular"
            android:id="@+id/editText_Celular"
            app:layout_constraintTop_toBottomOf="@+id/editText_Nome"
            android:layout_marginTop="72dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.181"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="Nome Completo"
            android:ems="10"
            android:id="@+id/editText_Nome"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.181" app:layout_constraintVertical_bias="0.052"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="phone"
            android:hint="Telefone Fixo"
            android:ems="10"
            android:id="@+id/editText_Telefone"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.181" app:layout_constraintBottom_toTopOf="@+id/editText_Celular"
            android:layout_marginBottom="16dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="EndereÃ§o"
            android:ems="10"
            android:id="@+id/editText_Endereco"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.181"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.063"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="246dp"/>
    <EditText
            android:layout_width="101dp"
            android:layout_height="42dp"
            android:inputType="textPersonName"
            android:hint="UF"
            android:ems="10"
            android:id="@+id/editText_UF"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.633" app:layout_constraintVertical_bias="0.378"
            app:layout_constraintTop_toBottomOf="@+id/editText_NumeroCasa"
            app:layout_constraintStart_toEndOf="@+id/editText_Cidade"/>
    <EditText
            android:layout_width="81dp"
            android:layout_height="45dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:hint="Numero"
            android:id="@+id/editText_NumeroCasa"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.437" app:layout_constraintVertical_bias="0.021"
            app:layout_constraintTop_toBottomOf="@+id/editText_NumeroDocumento"
            app:layout_constraintStart_toEndOf="@+id/editText_Endereco"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPostalAddress"
            android:ems="10"
            android:hint="CEP"
            android:id="@+id/editText_CEP"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.181" app:layout_constraintVertical_bias="0.049"
            app:layout_constraintTop_toBottomOf="@+id/editText_Endereco"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="Bairro"
            android:ems="10"
            android:id="@+id/editText_Bairro"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.181"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.072"
            app:layout_constraintTop_toBottomOf="@+id/editText_CEP"/>
    <Button
            android:text="Cadastrar"
            android:layout_width="166dp"
            android:layout_height="48dp" android:id="@+id/buttonCadastrarNovo"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.497" app:layout_constraintVertical_bias="0.631"
            app:layout_constraintTop_toBottomOf="@+id/editText_Cidade"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:hint="Cidade"
            android:ems="10"
            android:id="@+id/editText_Cidade"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintVertical_bias="0.115" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.181"
            app:layout_constraintTop_toBottomOf="@+id/editText_Bairro"/>
    <EditText
            android:layout_width="175dp"
            android:layout_height="45dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/editText_NumeroDocumento"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.779" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.032" app:layout_constraintTop_toBottomOf="@+id/editText_Celular"/>
    <TextView
            android:text="Data Nascimento :"
            android:layout_width="131dp"
            android:layout_height="28dp" android:id="@+id/textView10"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.128"
            app:layout_constraintVertical_bias="0.612"
            app:layout_constraintTop_toBottomOf="@+id/editText_Celular"
            app:layout_constraintBottom_toTopOf="@+id/editText_Endereco"/>

</androidx.constraintlayout.widget.ConstraintLayout>