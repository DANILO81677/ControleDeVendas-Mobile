<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.relatorios.DetalhamentoMensal">

    <com.google.android.material.tabs.TabLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.129"
            android:id="@+id/tabLayoutDetalhamentoMensal">

        <com.google.android.material.tabs.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Receitas"/>

        <com.google.android.material.tabs.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Pendente"/>

        <com.google.android.material.tabs.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Despesas"/>
        <com.google.android.material.tabs.TabItem
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/tabItem" android:text="Salarios"/>
    </com.google.android.material.tabs.TabLayout>
    <TextView
            android:text="TextView"
            android:layout_width="327dp"
            android:layout_height="29dp" android:id="@+id/textViewTotalDetalhamentoReceitas"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.38" app:layout_constraintVertical_bias="0.044"/>
    <TextView
            android:text="TextView"
            android:layout_width="327dp"
            android:layout_height="29dp" android:id="@+id/textViewTotalDetalhamentoPendente"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.404" app:layout_constraintVertical_bias="0.044"/>
    <TextView
            android:text="TextView"
            android:layout_width="327dp"
            android:layout_height="29dp" android:id="@+id/textViewTotalDetalhamentoDespesa"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.38" app:layout_constraintVertical_bias="0.044"/>
    <TextView
            android:text="TextView"
            android:layout_width="327dp"
            android:layout_height="29dp" android:id="@+id/textViewTotalDetalhamentoSalario"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.38" app:layout_constraintVertical_bias="0.044"/>
    <ListView
            android:layout_width="343dp"
            android:layout_height="559dp"
            android:id="@+id/lista_relatorio_detalhamento_mensal_receita"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
    <ListView
            android:layout_width="347dp"
            android:layout_height="560dp"
            android:id="@+id/lista_relatorio_detalhamento_mensal_despesa"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
    <ListView
            android:layout_width="346dp"
            android:layout_height="563dp"
            android:id="@+id/lista_relatorio_detalhamento_mensal_pendente"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
    <ListView
            android:layout_width="343dp"
            android:layout_height="559dp"
            android:id="@+id/lista_relatorio_detalhamento_mensal_salario"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>